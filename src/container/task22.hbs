<div>
    <div class="display-1 text-center">{{page_header.title}}</div>
    <p class="text-uppercase text-center">{{page_header.description}}</p>
    <div>
        {{#with section}}
        <div class="card">
            <div class="card-body">
                {{#with about_us}}
                <div class="display-6 text-end">{{title_list.main}}</div>
                <div class="text-end text-lowercase">{{title_list.sub}}</div>
                <button class="btn btn-info">{{button.text}}</button>
                {{/with}}
            </div>
        </div>
        <br />
        <br />
        {{#with advantage}}
        <div class="card">
            <div class="card-button">
                <div class="display-5">{{title}}</div>
                <div class="card">
                    <div class="card-body">
                        {{#with efficiency}}
                        <div class="display-4">{{title}}</div>
                        <div class="text-lowercase">{{sub_title}}</div>
                        <p class="text-muted">{{description}}</p>
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
                <br />
                <br />
                <div class="card">
                    <div class="card-body">
                        {{#with innovation}}
                        <div class="display-4 text-center">{{title}}</div>
                        <div class="text-lowercase text-center">{{sub_title}}</div>
                        <p class="text-muted text-center">{{description}}</p>
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
                <br />
                <br />
                <div class="card">
                    <div class="card-body">
                        {{#with collaboration}}
                        <div class="display-4 text-end">{{title}}</div>
                        <div class="text-lowercase text-end">{{sub_title}}</div>
                        <p class="text-muted text-end">{{description}}</p>
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
            </div>
        </div>
        {{/with}}
        <br />
        <br />
        {{#with agreement}}
        <div class="card">
            <div class="card-body">
                <div class="display-5">{{title}}</div>
                <div class="card">
                    <div class="card-body">
                        <div class="text-lowercase">{{info.sub_title}}</div>
                        <div class="card">
                            <div class="card-body">
                                <p class="text-muted text-lowercase">{{info.detail.text}}</p>
                                <button class="btn btn-success">{{info.detail.button}}</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{/with}}
        {{/with}}
    </div>
</div>